<div class="modal fade"
     id="createLogModal"
		 tabindex="-1"
		 role="dialog"
		 aria-labelledby="createLogModal"
		 aria-hidden="true">
	<div class="modal-dialog modal-dialog-centered" role="document">
		<div class="modal-content">
			<div class="modal-header">
				<h4 class="modal-title text-center" id="finishedModalCenterTitle">
          <div class="text-center">
          </div>
					Start the Timer
				</h4>
				<button type="button"
							  class="close"
								data-dismiss="modal"
								aria-label="Close">
					<span aria-hidden="true">&times;</span>
				</button>
			</div>
			<%= render "logs/form", log: log %>
		</div>
	</div>
</div>
